FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    outlet_up
    {
        type            freestreamPressure;
        freestreamValue uniform 101325;
        supersonic      false;
        value           uniform 101325;
    }
    outlet_down
    {
        type            freestreamPressure;
        freestreamValue uniform 101325;
        supersonic      false;
        value           uniform 101325;
    }
    airfoil
    {
        type            zeroGradient;
    }
    inlet
    {
        type            freestreamPressure;
        freestreamValue uniform 101325;
        supersonic      false;
        value           uniform 101325;
    }
    temp1
    {
        type            zeroGradient;
    }
    temp2
    {
        type            zeroGradient;
    }
}


