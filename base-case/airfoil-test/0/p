FoamFile

    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    b7-farfield_riem
    {
        type            freestreamPressure;
		freestreamValue	$internalField;
    }

    b4-farfield_riem
    {
        type            freestreamPressure;
		freestreamValue	$internalField;
    }

    b5-farfield_riem
    {
        type            freestreamPressure;
		freestreamValue	$internalField;
	}

    b8-to-stitch-a
    {
        type            zeroGradient;
    }

    defaultFaces
    {
        type            zeroGradient;
    }

    b6-viscous_solid
    {
        type            zeroGradient;
    }

    b2-symmetry_y_strong
    {
        type            symmetryPlane;
    }

    b3-symmetry_y_strong
    {
        type            symmetryPlane;
    }
