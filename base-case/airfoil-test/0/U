FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (25.75 3.62 0);

boundaryField
{
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    outlet_up
    {
        type            freestreamVelocity;
        freestreamValue $internalField;
    }
    outlet_down
    {
        type            freestreamVelocity;
        freestreamValue $internalField;
    }
    airfoil
    {
        type            noSlip;
    }
    inlet
    {
        type            freestreamVelocity;
        freestreamValue $internalField;
    }
    temp1
	{	
		type			zeroGradient;
	}
    temp2
	{	
		type			zeroGradient;
	}
}


// ************************************************************************* //
