FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      gamma;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1.0;

boundaryField
{
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    outlet_up
    {
        type            zeroGradient;
    }
    outlet_down
    {
        type            zeroGradient;
    }
    airfoil
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    temp1
	{	
		type			zeroGradient;
	}
    temp2
	{	
		type			zeroGradient;
	}
}


