FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1.547e-6;

boundaryField
{
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    outlet_up
    {
        type            freestream;
        freestreamValue $internalField;
    }
    outlet_down
    {
        type            freestream;
        freestreamValue $internalField;
    }
    airfoil
    {
        type            nutLowReWallFunction;
        value           uniform 0;
    }
    inlet
    {
        type            freestream;
        freestreamValue $internalField;
    }
    temp1
	{	
		type			zeroGradient;
	}
    temp2
	{	
		type			zeroGradient;
	}
}


