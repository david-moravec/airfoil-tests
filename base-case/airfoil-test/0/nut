FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1.547e-06;

boundaryField
{
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    outlet_up
    {
        type            freestream;
        freestreamValue uniform 1.547e-06;
        value           uniform 1.547e-06;
    }
    outlet_down
    {
        type            freestream;
        freestreamValue uniform 1.547e-06;
        value           uniform 1.547e-06;
    }
    airfoil
    {
        type            nutLowReWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0;
    }
    inlet
    {
        type            freestream;
        freestreamValue uniform 1.547e-06;
        value           uniform 1.547e-06;
    }
    temp1
    {
        type            zeroGradient;
    }
    temp2
    {
        type            zeroGradient;
    }
}


