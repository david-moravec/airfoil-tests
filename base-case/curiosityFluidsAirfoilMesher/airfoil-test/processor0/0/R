FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      R;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.14;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    farfield
    {
        type            freestream;
        freestreamValue uniform 0.14;
        value           uniform 0.14;
    }
    airfoil
    {
        type            fixedValue;
        value           uniform 0;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 0.14;
    }
}


