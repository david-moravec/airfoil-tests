FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      R;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 9.47e-4;

boundaryField
{
    farfield
    {
        type            freestream;
        freestreamValue $internalField;
    }
    airfoil
    {
        type            fixedValue;
        value           uniform 0;
    }

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
